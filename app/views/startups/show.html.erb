<div class="text-center">
  <h1><%= @startup.name %></h1>
  <p><%= @startup.description %></p>
  <div class="container">
    <div class="row">
      <div class="col-xs-6">
        <div class="show-box">
          <h2>Founders</h2>
          <% @startup.founders.each do |founder| %>
          <h2>founder.name</h2>
          <p>founder.email</p>
          <p>founder.phone</p>
          <% end %>
        </div>
      </div>
      <div class="col-xs-6">
        <div class="show-box">
          <h2>VC firms they raised with</h2>
        </div>

      </div>
      </div>
    </div>
  </div>
</div>
